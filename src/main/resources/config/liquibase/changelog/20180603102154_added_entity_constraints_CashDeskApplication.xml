<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity CashDeskApplication.
    -->
    <changeSet id="20180603102154-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="inventory_id"
                                 baseTableName="cash_desk_application"
                                 constraintName="fk_cash_desk_application_inventory_id"
                                 referencedColumnNames="id"
                                 referencedTableName="inventory"/>

        <addForeignKeyConstraint baseColumnNames="cash_desk_applications_id"
                                 baseTableName="cash_desk_application_banks"
                                 constraintName="fk_cash_desk_application_banks_cash_desk_applications_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cash_desk_application"/>
        <addForeignKeyConstraint baseColumnNames="banks_id"
                                 baseTableName="cash_desk_application_banks"
                                 constraintName="fk_cash_desk_application_banks_banks_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bank"/>
        
    </changeSet>
</databaseChangeLog>
